{
  "name": "kabutack",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite -c ./build/vite.config.dev.ts",
    "build:esm": "vite build -c ./build/vite.config.esm.ts",
    "build": "vuedx-typecheck . && vite build -c ./build/vite.config.esm.ts",
    "serve": "vite preview -c ./build/vite.config.dev.ts",
    "test": "jest",
    "lint": "eslint ./src  --ext .vue,.js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "cz": "git-cz --disable-emoji"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "async-validator": "^3.5.1",
    "feather-icons": "4.28.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vitejs/plugin-vue-jsx": "^1.0.2",
    "@vue/babel-plugin-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.0",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.5.0",
    "git-cz": "^4.7.6",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.1",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "mitt": "^2.1.0",
    "sass": "^1.32.8",
    "slugify": "^1.4.6",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "transliteration": "^2.2.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "ts-vue-plugin": "^0.1.3",
    "typescript": "^4.1.5",
    "vite": "^2.0.0",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
